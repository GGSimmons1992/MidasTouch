

@if ((string)ViewData["Title"] == "Home")
{
    <header class="bg-img rounded-bottom">
        <partial name="_NavBarPartial" />
    </header>
}
else if ((string)ViewData["Title"] == "Stock")
{
    <header class="bg-img-stock rounded-bottom">
        <partial name="_NavBarPartial" />
    </header>
}
else if ((string)ViewData["Title"] == "Stock Chart")
{
    <header class="bg-img-stockcharts rounded-bottom">
        <partial name="_NavBarPartial" />
    </header>
}
else if ((string)ViewData["Title"] == "Login")
{
    <header class="bg-img rounded-bottom">
        <partial name="_NavBarPartial" />
    </header>
}
else if ((string)ViewData["Title"] == "Register")
{
    <header class="bg-img rounded-bottom">
        <partial name="_NavBarPartial" />
    </header>
}

